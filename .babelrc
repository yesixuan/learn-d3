{
  "presets": [
    [
      "es2015",
      {
        "modules": false
      }
    ],
    "react",
    "stage-1"
  ],
  "plugins": [
    "transform-decorators-legacy",
      [
        "react-css-modules-modify",
        {
          "filetypes": {
            ".scss": {
              "syntax": "postcss-scss"
            }
          },
          "generateScopedName":"[name]__[local]___[hash:base64:5]"
      }
    ]
  ]
}
